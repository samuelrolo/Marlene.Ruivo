{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 256,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/og-image.jpg",
      "dest": "/public/og-image.jpg"
    },
    {
      "src": "/og-image.png",
      "dest": "/public/og-image.png"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
