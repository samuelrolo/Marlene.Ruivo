{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 256,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico|css|js|json|woff|woff2|ttf|eot))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
